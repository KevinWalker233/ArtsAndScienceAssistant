@media (prefers-color-scheme: dark) {
    page {
        background-color: rgb(0, 0, 0);
    }

    .headContent {
        background-color: #444;
        box-shadow: rgb(200, 200, 200);
    }

    .bodyCard {
        background-color: #444;
        box-shadow:0 0rpx 0rpx 0rpx rgb(243,243,243);
    }

    .viewContent {
        color: rgb(17, 17, 17);
    }

    .conternBox {
        color: rgb(17, 17, 17);
        background-color: rgb(78, 138, 214);
    }

    .title {
        color: white;
    }
}