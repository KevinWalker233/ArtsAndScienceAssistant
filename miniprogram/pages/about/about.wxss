.page {
  width: 100%;
  height: 100%;
  position: fixed;
  display: flex;
  background-color:rgb(56,120,194);
}

.card {
  width: 90%;
  height: 90%;
  position: fixed;
  left: 5%;
  right: 5%;
  border-radius: 15rpx;
  background-color: rgba(248, 248, 248, 1);
}

.h2{
  width: 90%;
  margin: 0 auto;
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(29,29,29,0.7);
}

.h3{
  width: 90%;
  margin: 0 auto;
  font-size: 20rpx;
  text-indent:2em;
}

