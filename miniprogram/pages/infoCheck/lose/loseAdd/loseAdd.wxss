page {
  background-color: rgb(243, 243, 243);
}

.card {
  width: 100%;
  background-color: white;
  margin: 20rpx 0rpx;
}

.user {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
}

.userButton {
  width: 200rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin: auto 10rpx;
  background-color: rgb(7, 193, 96);
  border-radius: 25rpx;
  text-align: center;
  font-size: 30rpx;
  color: white;
  font-weight: bold;
}

.map {
  width: 100%;
}

.text-cut {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}